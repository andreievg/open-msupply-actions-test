                    "rawQuery": true,
                    "rawSql": "SELECT\n    'Prescriber ' || ROW_NUMBER() OVER (ORDER BY COUNT(DISTINCT t.id) DESC) AS \"Prescriber\",\n    COUNT(DISTINCT t.id) AS \"Antibiotic Prescriptions\"\nFROM\n    transact t\n    JOIN trans_line tl ON t.id = tl.transaction_id\n    JOIN item i ON tl.item_id = i.id\n    LEFT JOIN clinician c ON t.prescriber_id = c.id\nWHERE\n    t.type = 'ci'\n    AND t.status IN ('fn', 'cn')\n    AND i.flags LIKE '%Antibiotic%'\n    AND t.confirm_date IS NOT NULL\n    AND t.confirm_date > $__timeFrom()\n    AND t.confirm_date < $__timeTo()\nGROUP BY\n    t.prescriber_id\nORDER BY\n    \"Antibiotic Prescriptions\" DESC\nLIMIT 10",
