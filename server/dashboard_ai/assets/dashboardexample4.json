                    "format": "table",
                    "group": [],
                    "metricColumn": "none",
                    "rawQuery": true,
                    "rawSql": "SELECT *\nFROM crosstab (\n    $$ SELECT \n        f_year AS year, \n        COALESCE(poc.description, 'No category') AS po_category, \n        COUNT(DISTINCT po.id) AS num \n    FROM purchase_order po \n    JOIN (\n      SELECT \n         CASE WHEN TO_CHAR(date,'MM')::integer > 6 \n        THEN CONCAT('1st July ', TO_CHAR(date,'YYYY'), ' - 30 June ',  TO_CHAR(date + interval '1 year','YYYY'))\n        ELSE CONCAT('1st July ', TO_CHAR(date - interval '1 year','YYYY'),' - 30 June ',  TO_CHAR(date,'YYYY')) END AS f_year, \n      TO_CHAR(date, 'YYYY/MM') yearmonth  \n      FROM (SELECT generate_series(CONCAT(TO_CHAR(current_date - interval '3 year', 'YYYY'),'07','01')::date , current_date, '1 month'::interval) AS date) years \n    ) fy ON fy.yearmonth = TO_CHAR(confirm_date, 'YYYY/MM') \n    JOIN store s ON po.store_id = s.id\n    JOIN purchase_order_line pol ON po.id = pol.purchase_order_id \n    LEFT JOIN purchase_order_category poc ON po.category_id = poc.id \n    WHERE pol.item_name IN (${item_name:sqlstring}) \n    AND s.name IN (${store_name:sqlstring}) \n    AND po.status IN ('cn', 'fn') \n    --AND $__timeFilter(confirm_date)\n    AND confirm_date > date_trunc('year', current_date - interval '3 years') \n    GROUP BY f_year, poc.description \n    ORDER BY f_year, poc.description $$ , \n    $$ SELECT description FROM purchase_order_category UNION SELECT 'No category' $$\n   ) AS t (\n        \"Year\" text,                   \n        \"RFQ\" int, \n        \"Tender\" int, \n        \"No category\" int\n    );",
                    "refId": "A",
                    "select": [
                        [
                            {
                                "params": [
                                    "value"
                                ],
                                "type": "column"
                            }
                        ]
                    ],
                    "timeColumn": "time",
                    "where": [
                        {
                            "name": "$__timeFilter",
                            "params": [],
                            "type": "macro"
                        }
                    ]
                }
            ],
            "type": "barchart"
        },
